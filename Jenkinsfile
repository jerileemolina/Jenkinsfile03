pipeline {
    agent any
    stages {
        stage ('Ejercicio3') {
            steps {
                sh '''
                read -p "Introduce la versión que quieres modificar:" version
                echo "Has elegido la versión $version"
                if ((version == 0.0.5)) {
                    echo " La versión actual de APP_JAVA_INT es $version"
                }
                done
                    '''
                }
            }
        }
    }